<%= link_to 'トップ画面に戻る', root_path %>
<%= link_to 'マニュアル詳細画面に戻る', manual_path(@manual) %>
<h1 style="font-size: 30px;">マニュアル編集画面</h1>
<%= form_with model: @manual, local: true do |f| %>
  <%= render 'shared/error_messages', model: f.object %>
  <%= render 'shared/form_manual', f: f %>

  <div>
    <%= f.submit '編集を完了する' %>
  </div>

<% end %>